<div class="container text-center">
  <h1 class="text-center">Themes</h1>
  <a href="/theme/add/" class="btn btn-danger">Create</a>
  <hr>
  <div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-10">
      <% _.each(themes, function (theme) { %>
        <div class="panel panel-default">
          <div class="panel-body">
            <h3 class="text-center"><%= theme.name %></h3>
       
          </div>
          <div class="panel-footer">
            <a href="/theme/watch/?id=<%= theme.id %>" class="btn btn-info">LEARN MORE</a>
            <hr>

          </div>
        </div>
      <% }) %>
    </div>
    <div class="col-md-1">
    </div>
    <ul class="pagination">
      <li><a href="/theme">1</a></li>
      <li><a href="/theme/2">2</a></li>
      <li><a href="/theme/3">3</a></li>
    </ul>
  </div>
</div>